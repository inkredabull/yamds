    <div class="col-lg-12">
      <h1>Fave tracks</h1>
      <hr>
      <% @liked_artists.sample(10).each do |a| %>
        <p><%= a[0] %></p>
      <% end %>
      <p>...</p>
      <hr>
      <p>
        <%= form_tag("/create_job", method: "post") do %>
